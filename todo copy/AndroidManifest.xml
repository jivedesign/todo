<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.jivedesign.todo"
    android:versionCode="1"
    android:versionName="1.0" >

     <uses-permission android:name="android.permission.WRITE_INTERNAL_STORAGE" />
    
    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="17" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="jhn-TodoApp"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.jivedesign.todo.MainActivity"
            android:label="jhn-TodoApp" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.jivedesign.todo.ArchiveActivity"
            android:label="Archive"></activity>
        <activity android:name="com.jivedesign.todo.ShareActivity"
            android:label="Share"></activity>
        <activity android:name="com.jivedesign.todo.ShareTodoActivity"
            android:label="Select Todos tasks to share"></activity>
        <activity android:name="com.jivedesign.todo.ShareArchiveActivity"
            android:label="Select Archives to share"></activity>
    </application>

</manifest>
